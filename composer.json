{
    "name": "rollerworks/version",
    "description": "Semantic versioning helper library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^7.1",
        "composer/semver": "^1.4.2"
    },
     "require-dev": {
         "phpunit/phpunit": "^7.3.2",
         "phpstan/phpstan": "^0.10.3",
         "phpstan/phpstan-phpunit": "^0.10",
         "symfony/phpunit-bridge": "^4.1"
     },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Version\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Version\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
